/* pages/videoCase/video.wxss */
.video_box {
    padding: 30rpx;
    flex-wrap: wrap;
    justify-content: flex-start;
}
.video_item {
    width: 50%;
    padding-bottom: 30rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
}
.video_img {
    width: 300rpx;
    height: 225rpx;
    position: relative;
}
.video_img image:not(.icon) {
    width: 100%;
    height: 100%;
}
.video_item .icon {
    position: absolute;
    top: 50%;
    left: 50%;
    width: 66rpx;
    height: 66rpx;
    margin-left:-33rpx;
    margin-top:-33rpx;
}
.video_item .title {
    padding-top: 12rpx;
}

