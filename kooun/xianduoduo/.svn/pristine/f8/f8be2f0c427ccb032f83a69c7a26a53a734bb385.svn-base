body {
	background-color: #F5F5F5;
}

.layui-tab-title li {
	padding: 0 20px 0 6px;
}
.layui-tab-title li .layui-tab-close {
	margin-left: 0;
	position: absolute;
}

.content {
	padding: 20px;
	background-color: #fff;
	margin: 20px;
}

/*数据列表查看图片（大图）*/
#previewImgBox {
	display: flex;
	justify-content: center;
	align-items: center;
}

/*上传图片*/
.layui-upload-img, .layui-upload-list img {
	margin: 0 10px 10px 0;
	width: 150px;
	border: 1px solid #e5e5e5;
}

.layui-upload-list{
	width: 850px;
	display:flex;
	align-items:center;
	flex-wrap: wrap;
}

