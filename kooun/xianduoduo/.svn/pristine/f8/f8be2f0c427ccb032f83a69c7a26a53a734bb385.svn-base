.layui-form-item .layui-input:hover, .layui-form-item .layui-textarea:hover
	{
	border-color: #01AAED;
}



/*page分页工具固定底部位置显示*/
.layui-table-page {
	background: #FFFFFF;
	position: fixed;
	bottom: 0;
	z-index: 19890000;
}
.layui-table img {
	width: 42px !important;
}

/* 流加载相关自定义样式 */
.select-input {
	width: 300px;
}

.layui-form .select-box {
	position: relative;
}

.select-box .layui-icon {
	position: absolute;
	top: 11px;
	right: 10px;
}

.select-box input {
	cursor: pointer;
}

.flow_box {
	margin-top: 38px;
	width: 190px;
	min-height: 150px;
	max-height: 200px;
	border: 1px solid #CCCCCC;
	overflow-y: scroll;
	text-align: center;
	display: none;
	position: absolute;
	left: 160px;
	z-index: 3;
	background-color: #fff;
}

.flow_box p {
	height: 36px;
	line-height: 36px;
	font-size: 14px;
	padding: 0 20px;
}

.flow_box p:hover {
	background-color: #e5e5e5;
	cursor: pointer;
}

.flow_box .active {
	background-color: #5FB878;
}
.flow_box .layui-form-checkbox {
	display: flex;
}
.export_list {
	width: 140px;
	right: 0;
	left: auto;
	margin-top: 0;
	overflow-y: hidden;
	min-height: inherit;
	z-index: 888;
}


	/* 自定义添加属性标签样式 */
.add-model {
	padding-left: 80px;
}

.model-box {
	margin-top: 20px;
	display: flex;
	align-items: center;
}

