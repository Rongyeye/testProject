.layui-form-label {
	width: 120px;
	position: relative;
}
.layui-input-block {
	margin-left: 150px;
	position: relative;
}
.layui-input-block .inputNumTip, .textareaNumTip {
	position: absolute;
	right: 50%;
	bottom: 0;
	padding: 11px 6px;
	color: #999;
}
.layui-input-block .textareaNumTip {
	right: 30%;
}
.layui-input-block .layui-input {
	width: 50%;
}
.layui-input-block .layui-form-select {
	width: 50%;
}
.layui-input-block .layui-textarea {
	width: 70%;
}
.layui-input-block .layui-input-inline .layui-form-select {
	width: 100%;
}
.layui-input-block .layui-form-select .layui-input {
	width: 100%;
}

.layui-input-block .layui-input-inline .layui-input {
	width: 230px;
}
.note-editor.note-frame {
	width: 80% !important;
}
.layui-form-item .layui-input-inline {
	/*width: 50%;*/
	margin-left: 30px;
}
.layui-input-block .layui-input-inline {
	margin-left: 0;
}
.layui-tab-title li {
	border-left: 1px solid #e5e5e5;
}

.layui-tab-title li:first-child {
	padding-right: 15px;
	border-left: none;
}

.layui-tab-title li:first-child .layui-tab-close {
	display: none;
}

.demoTable {
	display: flex;
	justify-content: space-between;
	align-items: center;
}

.dispFlexEnd {
	display: flex;
	justify-content: flex-end;
}

/*弹框关闭icon*/
.layui-layer-setwin .layui-layer-close2 {
	border-radius: 50%;
}

.layui-tab-content {
	min-height: 150px;
}

