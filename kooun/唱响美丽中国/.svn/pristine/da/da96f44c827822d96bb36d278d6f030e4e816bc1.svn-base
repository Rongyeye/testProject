/* pages/XExamineSuccess/XExamineSuccess.wxss */
page{
  background-color: rgb(245, 245, 245);
}

.frame{
  padding-bottom: 30rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  background-color: rgb(255, 255, 255);
}
.tip{
  margin-top: 29rpx;
  align-self: flex-start;
  margin-left: 20rpx;
  font-family: PingFang-SC-Medium;
  font-size: 28rpx;
  color: #010101;
}
.picture{
  margin-top: 29rpx;
  width: 620rpx;
  height: 367rpx;
}
.frame_item{
  width: 750rpx;
  margin-top: 20rpx;
  display: flex;
  flex-direction: column;
  background-color: rgb(255, 255, 255);
  border-bottom: 1rpx solid #e5e5e5;
}
.item{
  padding-top: 30rpx;
  padding-bottom: 30rpx;
  display: flex;
  border-top: 1rpx solid #e5e5e5;
}
.textA{
  margin-left: 19rpx;
  font-family: PingFang-SC-Medium;
  font-size: 30rpx;
  color: #333333;
}
.textB{
  margin-left: 30rpx;
  font-family: PingFang-SC-Medium;
  font-size: 30rpx;
  color: #666666;
  }
 .itemB{
   padding-top: 30rpx;
   padding-bottom: 30rpx;
   display: flex;
   align-items: center;
   justify-content: space-between;
   border-top: 1rpx solid #e5e5e5;
 }
 .text_type{
  justify-self: flex-end;
  margin-right: 30rpx;
  font-family: PingFang-SC-Medium;
  font-size: 30rpx;
  color: #666666;
 }
 .text_adress{
  margin-top: 25rpx;
  align-self: flex-start;
  margin-left: 20rpx;
  font-family: PingFang-SC-Medium;
  font-size: 30rpx;
  color: #666666;
 }
 .text_frame{
   flex-wrap: wrap;
   border-top: 1rpx solid #e5e5e5;
   width: 750rpx;
   height: 132rpx;
   display: flex;
 }
 .button{
   margin-top: 29rpx;
   margin-bottom: 28rpx;
   width: 670rpx;
   height: 94rpx;
   background-color: #ed4849;

   display: flex;
   justify-content: center;
   align-items: center;
   color: white;
   font-family: PingFang-SC-Bold;
   font-size: 36rpx;
 }